  "color": "#009fdf",
  "category": "anime",
  "tags": [
    "manga",
    "reading"
  ],
  "settings": [
    {
      "id": "lang",
      "multiLanguage": true
    },
    {
      "id": "privacy",
      "title": "Privacy",
      "icon": "fad fa-user-secret",
      "value": false
    },
    {
      "id": "cover",

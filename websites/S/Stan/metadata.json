{
  "$schema": "https://schemas.premid.app/metadata/1.13",
  "apiVersion": 1,
  "author": {
    "name": "SlowLife",
    "id": "374905512661221377"
  },
  "service": "Stan",
  "description": {
    "en": "Stan offers unlimited access to thousands of hours of entertainment, first-run exclusives, award-winning TV shows, blockbuster movies and kids content.",
    "nl": "Stan biedt onbeperkte toegang tot duizenden uren amusement, exclusieve primeurs, bekroonde tv-programma's, kaskrakerfilms en inhoud voor kinderen."
  },
  "url": "play.stan.com.au",
  "version": "1.1.20",
  "logo": "https://cdn.rcd.gg/PreMiD/websites/S/Stan/assets/logo.png",
  "thumbnail": "https://cdn.rcd.gg/PreMiD/websites/S/Stan/assets/thumbnail.png",
  "color": "#0173fa",
  "category": "videos",
  "tags": [
    "drama",
    "movie"
  ],
  "settings": [
    {
      "id": "lang",
      "multiLanguage": true
    },
    {
      "id": "privacy",
      "title": "Privacy Mode",
      "icon": "fad fa-user-secret",
      "value": false
    },
    {
      "id": "seriesDetail",
      "if": {
        "privacy": false
      },
      "title": "Series Details row1",
      "icon": "fas fa-comment-alt-edit",
      "value": "%title%",
      "placeholder": "Use %title% or %episode%"
    },
    {
      "id": "seriesState",
      "if": {
        "privacy": false
      },
      "title": "Video Details row2",
      "icon": "fas fa-comment-alt-edit",
      "value": "%episode%",
      "placeholder": "Use %title% or %episode%"
    },
    {
      "id": "timestamp",
      "title": "Show Timestamps",
      "icon": "fad fa-stopwatch",
      "value": true
    },
    {
      "id": "buttons",
      "if": {
        "privacy": false
      },
      "title": "Show Buttons",
      "icon": "fas fa-compress-arrows-alt",
      "value": true
    },
    {
      "id": "cover",
      "if": {
        "privacy": false
      },
      "title": "Show Cover",
      "icon": "fad fa-images",
      "value": true
    }
  ]
}

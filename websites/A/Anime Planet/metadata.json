{
  "$schema": "https://schemas.premid.app/metadata/1.13",
  "apiVersion": 1,
  "author": {
    "name": "SlowLife",
    "id": "374905512661221377"
  },
  "service": "Anime Planet",
  "description": {
    "en": "Anime and manga recommendations, reviews and more! Keep track of what you've seen in your own anime and manga list and make new friends.",
    "ga": "Moltaí anime agus manga, léirmheasanna agus go leor eile! Coinnigh súil ar an méid a chonaic tú i do liosta anime agus manga féin agus déan cairde nua.",
    "nl": "Aanbevelingen voor anime en manga, recensies en meer! Houd bij wat je hebt gezien in je eigen anime- en mangalijst en maak nieuwe vrienden.",
    "vi": "Giới thiệu những bộ anime và manga mới, đánh giá và hơn nữa! Theo dõi những gì bạn đã xem trong chính danh sách anime và manga của riêng bạn và gặp gỡ những người bạn mới."
  },
  "url": "www.anime-planet.com",
  "version": "1.1.31",
  "logo": "https://cdn.rcd.gg/PreMiD/websites/A/Anime%20Planet/assets/logo.jpg",
  "thumbnail": "https://cdn.rcd.gg/PreMiD/websites/A/Anime%20Planet/assets/thumbnail.png",
  "color": "#ec554a",
  "category": "anime",
  "tags": [
    "anime",
    "anime-planet"
  ],
  "iframe": true,
  "iFrameRegExp": ".*",
  "settings": [
    {
      "multiLanguage": true,
      "id": "lang"
    },
    {
      "id": "AnimeDetails",
      "title": "Anime Details row1",
      "icon": "fas fa-pen-square",
      "value": "%title%",
      "placeholder": "Use %title%, %episode%"
    },
    {
      "id": "AnimeState",
      "title": "Anime Details row2",
      "icon": "fas fa-pen-square",
      "value": "%episode%",
      "placeholder": "Use %title%, %episode%"
    },
    {
      "id": "MangaDetails",
      "title": "Manga Details row1",
      "icon": "fas fa-pen-square",
      "value": "%title%",
      "placeholder": "Use %title%, %chapter%"
    },
    {
      "id": "MangaState",
      "title": "Manga Details row2",
      "icon": "fas fa-pen-square",
      "value": "%chapter%",
      "placeholder": "Use %title%, %chapter%"
    },
    {
      "id": "buttons",
      "title": "Show Buttons",
      "icon": "fas fa-compress-arrows-alt",
      "value": true
    },
    {
      "id": "timestamp",
      "title": "Show Timestamps",
      "icon": "fad fa-stopwatch",
      "value": true
    }
  ]
}

{
	"$schema": "https://schemas.premid.app/metadata/1.11",
	"apiVersion": 1,
	"author": {
		"name": "Bas950",
		"id": "241278257335500811"
	},
	"service": "ListenOnRepeat",
	"description": {
		"en": "Discover music, check out the latest trending videos, and 24/7 curated content! Try ListenOnRepeat today.",
		"nl": "Ontdek muziek, bekijk de nieuwste trending video's en 24/7 samengestelde inhoud! Probeer ListenOnRepeat vandaag nog."
	},
	"url": "listenonrepeat.com",
	"version": "2.0.24",
	"logo": "https://cdn.rcd.gg/PreMiD/websites/L/ListenOnRepeat/assets/logo.png",
	"thumbnail": "https://cdn.rcd.gg/PreMiD/websites/L/ListenOnRepeat/assets/thumbnail.png",
	"color": "#ff0066",
	"category": "videos",
	"tags": [
		"listen",
		"repeat"
	],
	"iframe": true,
	"iFrameRegExp": ".*",
	"settings": [
		{
			"id": "sGlobalRepeat",
			"title": "Show global repeats",
			"icon": "fas fa-info",
			"value": false
		},
		{
			"id": "sFormatRepeat",
			"if": {
				"sGlobalRepeat": false
			},
			"title": "Display Format",
			"icon": "fas fa-paragraph",
			"value": "%repeatm%: %repeats%",
			"placeholder": "use: %repeatm% %repeats%"
		},
		{
			"id": "sFormatGlobalRepeat",
			"if": {
				"sGlobalRepeat": true
			},
			"title": "Display Format (With global repeats)",
			"icon": "fas fa-paragraph",
			"value": "%repeatm%: %repeats%, %grepeatm%: %grepeats%",
			"placeholder": "use: %repeatm% %repeats% %grepeatm% %grepeats%"
		}
	]
}
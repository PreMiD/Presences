{
  "$schema": "https://schemas.premid.app/metadata/1.6",
  "author": {
    "name": "Anderson Paiva",
    "id": "584828331954929909"
  },
  "service": "Booyah",
  "description": {
    "en": "BOOYAH! is a leading livestreaming and community platform designed for gaming enthusiasts, powered by Garena."
  },
  "url": "booyah.live",
  "version": "1.0.0",
  "logo": "https://i.imgur.com/Iv1PUYy.jpeg",
  "thumbnail": "https://i.imgur.com/tF1F2fi.png",
  "color": "#ffc106",
  "category": "videos",
  "tags": [
    "video",
    "streaming",
    "entertainment"
  ],
	"settings": [
		{
			"id": "lang",
			"multiLanguage": true
		},
		{
			"id": "privacy",
			"title": "Privacy Mode",
			"icon": "fad fa-user-secret",
			"value": false
		},
		{
			"id": "streamDetail",
			"if": {
				"privacy": false
			},
			"title": "Stream Details row1",
			"icon": "fas fa-comment-alt-edit",
			"value": "%title%",
			"placeholder": "Use %title% or %streamer% or %game%"
		},
		{
			"id": "streamState",
			"if": {
				"privacy": false
			},
			"title": "Stream Details row2",
			"icon": "fas fa-comment-alt-edit",
			"value": "%streamer%",
			"placeholder": "Use %title% or %streamer% or %game%"
		},
		{
			"id": "vidDetail",
			"if": {
				"privacy": false
			},
			"title": "Video Details row1",
			"icon": "fas fa-comment-alt-edit",
			"value": "%title%",
			"placeholder": "Use %title% or %uploader%"
		},
		{
			"id": "vidState",
			"if": {
				"privacy": false
			},
			"title": "Video Details row2",
			"icon": "fas fa-comment-alt-edit",
			"value": "%uploader%",
			"placeholder": "Use %title% or %uploader%"
		},
		{
			"id": "timestamp",
			"title": "Show Timestamps",
			"icon": "fad fa-stopwatch",
			"value": true
		},
		{
			"id": "profilePic",
			"title": "Show Streamer's Picture",
			"icon": "fad fa-images",
			"value": false
		},
		{
			"id": "buttons",
			"if": {
				"privacy": false
			},
			"title": "Show Buttons",
			"icon": "fas fa-compress-arrows-alt",
			"value": true
		}
	]
}
{
	"name": "presences",
	"version": "3.0.0",
	"author": "Timeraa",
	"license": "MPL-2.0",
	"scripts": {
		"lint": "prettier --write . && eslint --fix .",
		"lint:ci": "prettier --check . && eslint .",
		"format": "yarn ms && yarn lint",
		"se": "yarn format && yarn bumpChanged uncommitted",
		"compileTools": "tsc -p util/tools/tsconfig.json",
		"inc": "yarn compileTools && node ./util/tools/massVer.js",
		"bumpChanged": "yarn compileTools && node ./util/tools/bumpChanged.js",
		"sv": "yarn compileTools && node ./util/tools/auto/schemaValidator.js",
		"pu": "yarn compileTools && node ./util/tools/auto/presenceUpdater.js",
		"ms": "yarn compileTools && node ./util/tools/metadataSorter.js",
		"compile": "yarn compileTools && node ./util/tools/compileChanged.js",
		"translator": "yarn compileTools && node ./util/tools/translator.js",
		"prepare": "is-ci || husky install",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.9",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-angular": "^17.0.3",
		"@types/babel__core": "^7.1.19",
		"@types/chrome": "^0.0.193",
		"@types/debug": "^4.1.7",
		"@types/glob": "^7.2.0",
		"@types/json-to-ast": "^2.1.2",
		"@types/node": "^18.6.3",
		"@types/prettier": "^2.6.4",
		"@types/semver": "^7.3.10",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"axios": "^0.27.2",
		"chalk": "^4.1.2",
		"debug": "^4.3.4",
		"enquirer": "^2.3.6",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-no-one-time-vars": "^2.4.1",
		"eslint-plugin-unicorn": "^43.0.2",
		"glob": "^8.0.3",
		"husky": "^8.0.1",
		"is-ci": "^3.0.1",
		"json-to-ast": "^2.1.0",
		"jsonschema": "^1.4.1",
		"mongodb": "^4.4.1",
		"ora": "5.4.1",
		"prettier": "^2.7.1",
		"semver": "^7.3.7",
		"source-map-support": "^0.5.21",
		"terser": "^5.14.2",
		"typescript": "^4.8.2"
	}
}
